{
  "name": "rxwebsocket",
  "version": "1.0.7",
  "description": "RxJS WebSocket Advanced Wrapper",
  "main": "./dist/cjs/rxwebsocket.js",
  "module": "./dist/esm/rxwebsocket.js",
  "types": "./dist/esm/rxwebsocket.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/rxwebsocket.js",
      "import": "./dist/esm/rxwebsocket.js",
      "types": "./dist/esm/rxwebsocket.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format",
    "preversion": "npm run format"
  },
  "author": "Artico",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/art1c0/rxwebsocket.git"
  },
  "bugs": {
    "url": "https://github.com/art1c0/rxwebsocket/issues"
  },
  "homepage": "https://github.com/art1c0/rxwebsocket#readme",
  "funding": "https://buymeacoffee.com/artico",
  "keywords": [
    "rx",
    "rxjs",
    "websocket"
  ],
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "rxjs": "^7.8.2"
  },
  "files": [
    "dist/**/*"
  ]
}
